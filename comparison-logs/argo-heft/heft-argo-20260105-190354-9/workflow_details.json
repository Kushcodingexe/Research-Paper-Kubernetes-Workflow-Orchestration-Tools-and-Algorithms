                        "pip install -q kubernetes\n\n# Run HEFT scheduler (simplified inline version)\npython3 \u003c\u003c 'EOF'\nimport random\n\n# Node configuration\nnodes = {\n    'worker-w001': 'R1', 'worker-w002': 'R1',\n    'worker-w003': 'R2', 'worker-w004': 'R2',\n    'worker-w005': 'R3', 'worker-w006': 'R3',\n}\n\n# Simple HEFT: assign based on zone distribution for fault tolerance\nzones = ['R1', 'R2', 'R3']\n\n# Health checks spread across zones\nhc_nodes = ['worker-w001', 'worker-w003', 'worker-w005']  # One per zone\n\n# Simulation nodes - pick from different zones than health checks\nsim_node = 'worker-w002'  # R1 - execution node\nsim_zone = 'R1'\n\n# Rack simulation on different zone\nrack_node = 'worker-w004'  # R2\nrack_zone = 'R2'\n\n# Write outputs\nwith open('/tmp/heft-node-hc1', 'w') as f: f.write(hc_nodes[0])\nwith open('/tmp/heft-node-hc2', 'w') as f: f.write(hc_nodes[1])\nwith open('/tmp/heft-node-hc3', 'w') as f: f.write(hc_nodes[2])\nwith open('/tmp/heft-node-sim', 'w') as f: f.write(sim_node)\nwith open('/tmp/heft-zone-sim', 'w') as f: f.write(sim_zone)\nwith open('/tmp/heft-node-interim', 'w') as f: f.write('worker-w005')\nwith open('/tmp/heft-node-rack', 'w') as f: f.write(rack_node)\nwith open('/tmp/heft-zone-rack', 'w') as f: f.write(rack_zone)\nwith open('/tmp/heft-node-final', 'w') as f: f.write('worker-w006')\n\nprint(\"HEFT Schedule:\")\nprint(f\"  Health Checks: {hc_nodes}\")\nprint(f\"  Node Sim: {sim_node} (zone {sim_zone}) - EXCLUDE from failure targets\")\nprint(f\"  Rack Sim: {rack_node} (zone {rack_zone}) - EXCLUDE zone from failure targets\")\nEOF\n\necho \"HEFT initialization complete\"\n"
